@article{Riikonen2008,
abstract = {The long-term influence and contribution of research can be evaluated$\backslash$nrelatively reliably by bibliometric citation analysis. Previously,$\backslash$nproductivity of nations has been estimated by using either the number of$\backslash$npublished articles or journal impact factors and/or citation data. These$\backslash$nstudies show certain trends, but detailed analysis is not possible due$\backslash$nto the assumption that all articles in a journal were equally cited.$\backslash$nHere we describe the first comprehensive, longterm, nationwide analysis$\backslash$nof scientific performance. We studied the lifetime research output of$\backslash$n748 Finnish principal investigators in biomedicine during the years$\backslash$n1966-2000, analysed national trends, and made a comparison with$\backslash$ninternational research production. Our results indicate that analyses of$\backslash$nthe scientific contribution of persons, disciplines, or nations should$\backslash$nbe based on actual publication and citation counts rather than on$\backslash$nderived information like impact factors. 51{\%} of the principal$\backslash$ninvestigators have published altogether 75{\%} of the articles; however,$\backslash$nthe whole scientific community has contributed to the growth of$\backslash$nbiomedical research in Finland since the Second World War.},
author = {Riikonen, Pentti and Vihinen, Mauno},
doi = {10.1007/s11192-007-1962-y},
