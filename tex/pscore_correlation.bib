isbn = {0169-7552},
issn = {01697552},
journal = {Computer Networks and ISDN Systems},
keywords = {information retrieval,pagerank,search engines,world wide web},
number = {1/7},
pages = {107--17},
pmid = {726238533241861127},
title = {{The anatomy of a large scale hypertextual Web search engine}},
volume = {30},
